{
  "name": "@urban-ui/theme",
  "version": "0.1.0",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.tsx",
      "require": "./src/index.tsx"
    },
    "./build": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./atoms": {
      "types": "./dist/atoms.css.d.ts",
      "import": "./src/atoms.css.ts",
      "require": "./src/atoms.css.ts"
    },
    "./base": {
      "import": "./src/base.css.ts",
      "require": "./src/base.css.ts"
    },
    "./reset": {
      "import": "./src/reset.css.ts",
      "require": "./src/reset.css.ts"
    },
    "./system": {
      "import": "./src/system.css.ts",
      "require": "./src/system.css.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "clean": "del dist",
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "dependencies": {
    "@capsizecss/core": "^3.1.1",
    "@capsizecss/metrics": "^1.2.0",
    "@capsizecss/vanilla-extract": "^1.0.0",
    "@urban-ui/utils": "workspace:*",
    "@vanilla-extract/css": "^1.11.1",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "just-map-values": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react": "^4.0.1",
    "config-eslint": "workspace:*",
    "config-ts": "workspace:*",
    "del-cli": "^5.0.0",
    "eslint": "^8.46.0",
    "react": "^18.2.0",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "tiny-glob": "^0.2.9",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.2"
  }
}
