{
	"name": "@urban-ui/theme",
	"version": "0.4.0",
	"type": "module",
	"module": "./dist/index.js",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./build": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./styles": {
			"import": "./dist/style.css",
			"require": "./dist/style.css"
		},
		"./atoms": {
			"types": "./dist/atoms.css.d.ts",
			"import": "./dist/atoms.css.js",
			"require": "./dist/atoms.css.cjs"
		},
		"./base": {
			"types": "./dist/base.css.d.ts",
			"import": "./dist/base.css.js",
			"require": "./dist/base.css.cjs"
		},
		"./reset": {
			"import": "./dist/reset.css.js",
			"require": "./dist/reset.css.cjs"
		},
		"./system": {
			"types": "./dist/system.css.d.ts",
			"import": "./dist/system.css.js",
			"require": "./dist/system.css.cjs"
		}
	},
	"license": "MIT",
	"scripts": {
		"lint": "eslint \"**/*.ts*\"",
		"clean": "del dist",
		"dev": "vite build --watch",
		"build": "vite build"
	},
	"dependencies": {
		"@capsizecss/core": "^3.1.1",
		"@capsizecss/metrics": "^1.2.0",
		"@capsizecss/vanilla-extract": "^1.0.0",
		"@urban-ui/utils": "workspace:*",
		"@vanilla-extract/css": "^1.11.1",
		"@vanilla-extract/sprinkles": "^1.6.1",
		"just-map-values": "^3.2.0"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"config-eslint": "workspace:*",
		"config-ts": "workspace:*",
		"config-vite": "workspace:*",
		"del-cli": "^5.0.0",
		"eslint": "^8.46.0",
		"react": "^18.2.0",
		"typescript": "^5.1.3",
		"vite": "^5.0.10",
		"vitest": "^1.1.1"
	}
}
