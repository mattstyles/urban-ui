{
  "name": "@urban-ui/testx",
  "version": "0.0.1",
  "description": "stylex pattern exploration",
  "type": "module",
  "sideEffects": false,
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./build": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./anatomy": {
      "types": "./dist/anatomy.css.d.ts",
      "import": "./dist/anatomy.css.js",
      "require": "./dist/anatomy.css.cjs"
    }
  },
  "files": ["dist", "package.json", "readme.md"],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Matt Styles",
    "url": "https://github.com/mattstyles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/urban-ui.git",
    "directory": "packages/utility/testx"
  },
  "bugs": {
    "url": "https://github.com/mattstyles/urban-ui/issues"
  },
  "keywords": [
    "ui",
    "react",
    "components",
    "urban-ui",
    "react-aria",
    "vanilla-extract"
  ],
  "scripts": {
    "lint:eslint": "eslint src/*.ts*",
    "lint:biome": "biome lint src",
    "clean": "del dist",
    "dev": "vite build --watch",
    "build:vite": "vite build",
    "test:vitest": "vitest run src",
    "test:jest": "jest",
    "build:arc": "arc build",
    "build:swc": "swc src -d dist --config-file .swcrc-esm --strip-leading-paths",
    "build:swc:cjs": "swc src -d dist --config-file .swcrc-cjs --strip-leading-paths --out-file-extension cjs",
    "type:check": "tsc"
  },
  "dependencies": {
    "@react-aria/focus": "^3.13.0",
    "@react-aria/interactions": "^3.16.0",
    "@react-aria/utils": "^3.18.0",
    "@stylexjs/stylex": "0.9.3",
    "@urban-ui/button": "workspace:*",
    "@urban-ui/flex": "workspace:*",
    "@urban-ui/text": "workspace:*",
    "@urban-ui/theme": "workspace:*",
    "@urban-ui/arc": "workspace:*",
    "@urban-ui/utils": "workspace:*",
    "@vanilla-extract/css": "^1.11.1",
    "@vanilla-extract/css-utils": "^0.1.3",
    "clsx": "^2.0.0",
    "cva": "npm:class-variance-authority@^0.7.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@jest/globals": "^29.7.0",
    "@stylexjs/babel-plugin": "^0.5.1",
    "@stylexjs/dev-runtime": "^0.5.1",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.8",
    "@swc/jest": "^0.2.36",
    "@swc/plugin-transform-imports": "^1.5.119",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@urban-ui/tsconfig": "workspace:*",
    "config-vite": "workspace:*",
    "del-cli": "^5.1.0",
    "eslint": "^8.46.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.1.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1"
  }
}
