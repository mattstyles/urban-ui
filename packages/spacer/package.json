    "clean": "del dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run src"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "config-eslint": "workspace:*",
    "config-ts": "workspace:*",
    "config-vite": "workspace:*",
    "del-cli": "^5.0.0",
    "jsdom": "^21.1.0",
    "react-dom": "18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "clean": true,
    "target": "es2019",
