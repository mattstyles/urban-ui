{
  "name": "@urban-ui/spacer",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/@urban-ui/spacer/spacer.js",
  "module": "./dist/@urban-ui/spacer/spacer.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@urban-ui/spacer/spacer.js",
      "export": "./dist/@urban-ui/spacer/spacer.cjs"
    }
  },
  "license": "MIT",
  "author": "Matt Styles",
  "keywords": [
    "ui",
    "react",
    "components",
    "urban-ui",
    "stitches",
    "radix-ui"
