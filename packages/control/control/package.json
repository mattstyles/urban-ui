{
  "name": "@urban-ui/control",
  "version": "0.0.1",
  "description": "Composite package of all urban-ui control components",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/@urban-ui/control.js",
  "module": "./dist/@urban-ui/control.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@urban-ui/control.js",
      "export": "./dist/@urban-ui/control.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "Matt Styles",
    "url": "https://github.com/mattstyles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/urban-ui.git",
    "directory": "packages/components/control"
  },
  "bugs": {
    "url": "https://github.com/mattstyles/urban-ui/issues"
  },
  "keywords": [
    "ui",
    "react",
    "components",
    "urban-ui",
    "stitches",
    "radix-ui"
  ],
  "scripts": {
    "lint": "eslint *.ts*",
    "clean": "del dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run src"
  },
  "dependencies": {
    "@urban-ui/button": "workspace:*",
    "@urban-ui/button-group": "workspace:*",
    "@urban-ui/checkbox": "workspace:*",
    "@urban-ui/input": "workspace:*",
    "@urban-ui/theme": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@stitches/react": "^1.2.8",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.28",
    "@types/testing-library__jest-dom": "^5.14.5",
    "config-eslint": "workspace:*",
    "config-ts": "workspace:*",
    "config-vite": "workspace:*",
    "del-cli": "^5.0.0",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
