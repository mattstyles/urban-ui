{
  "extends": "config-ts/nextjs.json",
  "compilerOptions": {
    "plugins": [{"name": "next"}],
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "~/src/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
